{
  "name": "@outliner/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api-client": "openapi-ts -i http://127.0.0.1:8000/openapi.json -o src/api-client",
    "generate-api-client-test-port": "openapi-ts -i http://127.0.0.1:8001/openapi.json -o src/api-client",
    "test:comment": "To isolate db changes, tests are run on a different server on port 8001. Use make test from root to run.",
    "test:setup": "curl -X POST http://127.0.0.1:8001/databases -H \"Content-Type: application/json\" -d '{\"name\": \"playwright_test_db\"}'",
    "test": "npm run test:setup && npx playwright test --workers 1 --reporter=list --project='chromium'"
  },
  "dependencies": {
    "@mantine/core": "^8.3.4",
    "@mantine/hooks": "^8.3.4",
    "@mantine/notifications": "^8.3.5",
    "@tabler/icons-react": "^3.35.0",
    "@types/loglevel": "^1.5.4",
    "loglevel": "^1.9.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@hey-api/openapi-ts": "0.84.2",
    "@playwright/test": "^1.56.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
