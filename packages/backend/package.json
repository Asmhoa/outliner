  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.2",
    "@types/cors": "^2.8.12",
    "@types/better-sqlite3": "^7.6.12",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "concurrently": "^8.2.2"
